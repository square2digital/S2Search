{
  "ApplicationInsights": {
    "InstrumentationKey": "$env:INSTRUMENTATIONKEY"
  },
  "Logging": {
    "LogLevel": {
      "Default": "$env:DEFAULTLOGLEVEL",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "$env:DEFAULTLOGLEVEL",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "StorageQueue": "$env:AZURESTORAGEACCOUNT"
  },
  "AppSettings": {
    "DemoSearchCredentials": {
      "UseDemoSearchCredentials": "$env:USEDEMOSEARCHCREDENTIALS",
      "SearchCredentialsQueryKey": "$env:SEARCHCREDENTIALSQUERYKEY",
      "SearchCredentialsIndexName": "$env:SEARCHCREDENTIALSINDEXNAME",
      "SearchCredentialsInstanceEndpoint": "$env:SEARCHCREDENTIALSINSTANCEENDPOINT",
      "SearchCredentialsInstanceName": "$env:SEARCHCREDENTIALSINSTANCENAME"
    },
    "SearchSettings": {
      "UseRequestedCallingHost": "$env:USEREQUESTEDCALLINGHOST",
      "DefaultSearchOrderBy": "$env:DEFAULTSEARCHORDERBY",
      "FacetOrder": "$env:FACETORDER"
    },
    "MemoryCacheSettings": {
      "SearchCacheSlidingExpirySeconds": "$env:SEARCHCACHESLIDINGEXPIRYSECONDS",
      "ConfigCacheSlidingExpirySeconds": "$env:CONFIGCACHESLIDINGEXPIRYSECONDS",
      "DefaultFacetsCacheExpiryInSeconds": "$env:DEFAULTFACETSCACHEEXPIRYINSECONDS",
      "GenericSynonymsCacheExpiryInSeconds": "$env:GENERICSYNONYMSCACHEEXPIRYINSECONDS"
    },
    "RedisCacheSettings": {
      "RedisConnectionString": "$env:REDISCONNECTIONSTRING",
      "DefaultCacheExpiryInSeconds": "$env:DEFAULTCACHEEXPIRYINSECONDS"
    },
    "AdminSettings": {
      "AdminEndpoint": "$env:ADMINENDPOINT"
    }
  }
}
